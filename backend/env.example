# Dynamic Workflow System Configuration
# Copy this file to .env and fill in your actual values

# Application Settings
DEBUG=true
APP_NAME="Dynamic Workflow System"
APP_VERSION="0.1.0"

# CORS Settings - Update with your frontend URLs
CORS_ORIGINS=["http://localhost:3000", "http://localhost:3001"]

# Google Gemini API Configuration (choose one approach)
# Option 1: Use Gemini Developer API (simplest setup)
GOOGLE_API_KEY=your_google_api_key_here
GEMINI_MODEL=gemini-2.0-flash-001

# Option 2: Use Vertex AI (for enterprise environments)
# USE_VERTEX_AI=true
# GOOGLE_CLOUD_PROJECT=your-project-id
# GOOGLE_CLOUD_LOCATION=us-central1
# You'll also need to authenticate with gcloud or service account

# Web Search Configuration (optional)
# GOOGLE_API_KEY=your_google_api_key_for_search  # Can reuse same key
# GOOGLE_CSE_ID=your_custom_search_engine_id

# Workflow Settings
DEFAULT_WORKFLOW=orchestrator_workers
MAX_RETRIES=3
TIMEOUT_SECONDS=120

# File Storage Settings
SAVE_RESPONSES=true
CONTEXT_FILE_PATH=C:\Users\sidki\source\repos\effective\context.md

# Legacy Azure OpenAI Settings (deprecated - remove after migration)
# AZURE_OPENAI_API_KEY=your_azure_api_key
# AZURE_OPENAI_ENDPOINT=https://your-resource.openai.azure.com/
# AZURE_OPENAI_DEPLOYMENT_NAME=gpt-4
# AZURE_OPENAI_API_VERSION=2025-03-01-preview 